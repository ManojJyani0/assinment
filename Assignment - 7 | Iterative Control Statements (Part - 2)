1. Write a program to find the Nth term of the Fibonnaci series.
#include <stdio.h>
int main() {

  int i, n;
  int t1 = 0, t2 = 1;
  int nextTerm = t1 + t2;
  printf("\nEnter a number :");
  scanf("%d", &n);
  for (i = 3; i <= n; ++i)
  {    
    t1 = t2;
    t2 = nextTerm;
    nextTerm = t1 + t2;
  }
    printf("%d th term is :%d, ",n,nextTerm);
  return 0;
}
2. Write a program to print first N terms of Fibonacci series
#include <stdio.h>
int main() {

  int i, n;
  int t1 = 0, t2 = 1;
  int nextTerm = t1 + t2;
  printf("Enter the number of terms: ");
  scanf("%d", &n);
  printf("Fibonacci Series: %d, %d, ", t1, t2);
  for (i = 3; i <= n; ++i) {
    printf("%d, ", nextTerm);
    t1 = t2;
    t2 = nextTerm;
    nextTerm = t1 + t2;
  }

  return 0;
}
3. Write a program to check whether a given number is there in the Fibonacci series or not.
#include <stdio.h>
int main() {

  int i, input;
  int t1 = 0, t2 = 1;
  int nextTerm = t1 + t2;
  printf("Enter a number : ");
  scanf("%d", &input);
  if(t1==input||t2==input||0==input){
  printf("%d is belogns to Fibonnaci series",input);

  }else
  for (i = 3; i; ++i) {
    t1 = t2;
    t2 = nextTerm;
    nextTerm = t1 + t2;
    if(nextTerm==input){
      printf("%d is belogns to Fibonnaci series",input);
      break;
    }else if(input==i){
      printf("not belogns to fibonnaci series");
    break;
    }
  }
  return 0;
}
4. Write a program to calculate HCF of two numbers
#include <stdio.h>
int main()
{
    int l,a,b;
    printf("enter two numbers :");
    scanf("%d%d",&a,&b);
    for(l=a<b?b:a;l;l--){
    if(a%l==0 && b%l==0)
    break;
    }
    printf("HCF is %d\n",l);
    return 0;
}
5. Write a program to check whether two given numbers are co-prime numbers or not
#include <stdio.h>
int main()
{
    int l,a,b;
    printf("enter two numbers :");
    scanf("%d%d",&a,&b);
    for(l=a<b?b:a;l;l--){
    if(a%l==0 && b%l==0)
    break;
    }
    if(l==1)
    printf("Number are co-prime numbers");
    else
    printf("Number are not co-prime numbers");
    return 0;
}
6. Write a program to print all Prime numbers under 100
#include <stdio.h>
    int main(){
        int i,j;
        
        for(j=1;j<=100;j++)
        {   for(i=2;i<=j;i++){

                if(j%i==0)
                break;
            }
            if(i==j)
            printf("%d ",i);
        }
        return 0;
    }
7. Write a program to print all Prime numbers between two given numbers
#include <stdio.h>
    int main(){
        int i,j,l;
        printf("Enter two number :");
        scanf("%d %d",&j,&l);
        for(j;j<=l;j++)
        {   for(i=2;i<=j;i++){

                if(j%i==0)
                break;
            }
            if(i==j)
            printf("%d ",i);
        }
        return 0;
    }
8. Write a program to find next Prime number of a given number
#include <stdio.h>
    int main(){
        int i,j,l;
        printf("Enter a number :");
        scanf("%d",&j);
        for(j;j;j++)
        {   for(i=2;i<=j;i++){

                if(j%i==0)
                break;
            }
            if(i==j){
            printf("%d ",i);
            break;
            }
        }
        return 0;
    }
9. Write a program to check whether a given number is an Armstrong number or not
#include <stdio.h>
int main()
{
    int y,input,k,i,sum=0;
    printf("Enter a number:");
    scanf("%d",&input);
    y=input;
        for (i=1;i<4;i++)
        {
            k=input%10;
            sum=sum+k*k*k;
            input=input/10;
        }

        if(y==sum){
            printf(" armstrong number");
        }
        else printf("not a armstrong number");
        return 0;
}
