1.  // Write a program to print MySirG N times on the screen.
    #include <stdio.h>
    int main()
    {
        int input;
        printf("Enter a number :")
        scanf("%d",&input);
        while(input--)
        printf("MySirG\n");
        return 0;
    }

2.  // Write a program to print the first N natural numbers.
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Eanter a natural number :");
        scanf("%d",&input);
        for (i=1;i<=input;i++)
        printf("%d ",i);
        return 0;
    }

3.  // Write a program to print the first N natural numbers in reverse order.
    #include <stdio.h>
    int main()
    {
        int input;
        printf("Enter a number :");
        scanf("%d", &input);
        while (input)
        printf("%d ",input--);
        return 0;
    }

4.  //Write a program to print the first N odd natural numbers
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Enter a number : ");
        scanf("%d", &input);
        for(i=1;i<=input*2;i++)
        {
            if(i%2)
            printf("%d ",i);
        }
        return 0;
    }

5.  //Write a program to print the first N odd natural numbers in reverse order.
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Enter a number : ");
        scanf("%d", &input);
        for(i=input*2;i;i--)
        {
            if(i%2)
            printf("%d ",i);
        }
        return 0;
    }

6.  //Write a program to print the first N even natural numbers
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Enter a number : ");
        scanf("%d", &input);
        for(i=1;i<=input*2;i++)
        {
            if(!(i%2))
            printf("%d ",i);
        }
        return 0;
    }

7.  //Write a program to print the first N even natural numbers in reverse order
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Enter a number : ");
        scanf("%d", &input);
        for(i=input*2;i;i--)
        {
            if(!(i%2))
            printf("%d ",i);
        }
        return 0;
    }

8.  //Write a program to print squares of the first N natural numbers
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Eanter a  number :");
        scanf("%d",&input);
        for (i=1;i<=input;i++)
        printf("%d ",i*i);
        return 0;
    }

9.  //Write a program to print cubes of the first N natural numbers
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Eanter a  number :");
        scanf("%d",&input);
        for (i=1;i<=input;i++)
        printf("%d ",i*i*i);
        return 0;
    }

10. //Write a program to print a table of N.
    #include <stdio.h>
    int main()
    {
        int i,input;
        printf("Eanter a  number :");
        scanf("%d",&input);
        for (i=1;i<=10;i++)
        printf("%d ",input*i);
        return 0;
    }

