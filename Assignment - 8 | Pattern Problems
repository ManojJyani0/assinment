7.
#include <stdio.h>
int main(void)
{
    int i,j;
    for (i=5;i;i--)
    {
        for(j=1;j<=10;j++)
        {
        if(j<=i||j>=11-i)
        printf("*");
        else
        printf(" ");
        }
        printf("\n");
    }

return 0;
}

8.
#include <stdio.h>
int main(void)
{
    int i,j,k;
    for (i=4;i;i--)
    {   k=1;
        for(j=1;j<=7;j++)
        {
            
        if(j>=i&&8-i>=j){
        printf("%d",k);
        j<4?k++:k--;
        }
        else
        printf(" ");
        
        }
        printf("\n");
    }

return 0;
}

