4.
#include <stdio.h>
int main(void)
{
    int i,j;
    for (i=5;i;i--)
    {
        for(j=1;j<=9;j++)
        {
        if(j>=i&&j<11-i)
        printf("*");
        else
        printf(" ");
        }
        printf("\n");
    }

return 0;
}

