18.
#include <stdio.h>
int main(void)
{
    int i,j;
    for (i=5;i;i--)
    {
        for(j=1;j<=9;j++)
        {
        if(j>=i&&j<11-i)
        printf("*");
        else
        printf(" ");
        }
        printf("\n");
    }
    for (i=1;i<=4;i++)
    {   printf(" ");
        for(j=1;j<=7;j++)
        {
        if(j>=i&&j<=8-i)
        printf("*");
        else
        printf(" ");
        }
        printf("\n");
    }


return 0;
}

19.
#include <stdio.h>
int main(void)
{
    int i,j;
    for(i=i;i<=4;i++)
    {
        for(j=1;j<=19;j++)
        if ((4-i<=j&&6+i>j)||(14-i<=j&&16+i>=j))
        {
            if(i==4&&j==8)
            printf("MySirG");
            else if(i==4&&j==15)
            break;
            else
            printf("*");
        }
        else
        printf(" ");
        printf("\n");
    }
    for (i=1;i<=9;i++)
    {   printf(" ");
        for(j=1;j<=17;j++)
        {
        if(j>=i&&j<=18-i)
        printf("*");
        else
        printf(" ");
        }
        printf("\n");
    }

return 0;
}
