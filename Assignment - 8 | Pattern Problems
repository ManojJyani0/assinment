    }

return 0;
}

11.
#include <stdio.h>
int main(void)
{
    int i,j,k;
    for (i=5;i;i--)
    {   k=65;
        for(j=1;j<=9;j++)
        {
            
        if(j>=i&&10-i>=j){
        printf("%c",k);
        j<5?k++:k--;
        }
        else
        printf(" ");
        
        }
        printf("\n");
    }

return 0;
}

